{
  "name": "nextjs-openid-mongodb",
  "version": "0.6.0",
  "description": "Next.js SDK for signing in with OpenID and mongodb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.browser.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": "^10.13.0 || >=12.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "pretty": "prettier --write \"src/**/*.ts\" \"src/*.ts\"",
    "lint": "eslint --fix --ext .ts ./src",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "prepublishOnly": "npm test && npm run lint",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggallon/nextjs-openid-mongodb.git"
  },
  "keywords": [
    "openid",
    "next.js",
    "react",
    "oidc",
    "authentication",
    "mongodb",
    "zeit"
  ],
  "author": "GwenaÃ«l ggallon <gwenael.gallon@mac.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ggallon/nextjs-openid-mongodb/issues"
  },
  "homepage": "https://github.com/ggallon/nextjs-openid-mongodb#readme",
  "devDependencies": {
    "@panva/jose": "1.9.2",
    "@types/cookie": "0.3.3",
    "@types/hapi__iron": "5.1.0",
    "@types/jest": "24.0.21",
    "@types/jsonwebtoken": "8.3.5",
    "@types/node": "12.12.3",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/request": "2.48.3",
    "@typescript-eslint/eslint-plugin": "2.6.0",
    "@typescript-eslint/parser": "2.6.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.9.0",
    "next": "9.1.1",
    "nock": "11.7.0",
    "prettier": "1.18.2",
    "request": "2.88.0",
    "timekeeper": "2.2.0",
    "ts-jest": "24.1.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "@hapi/iron": "5.1.4",
    "base64url": "3.0.1",
    "cookie": "0.4.0",
    "openid-client": "3.7.4"
  },
  "peerDependencies": {
    "next": "^9.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/",
      "./jest.config.js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "preset": "ts-jest"
  }
}
